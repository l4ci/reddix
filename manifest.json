{
  "manifest_version": 2,
  "name": "Reddix",
  "version": "0.3",
  "description": "Clean and simple reddit chrome extension",

  "author": "Volker Otto",
  "content_scripts": [
    {
      "matches": ["*://www.reddit.com/*"],
      "css": ["main.css"],
      "js": [
              "js/jquery-2.0.3.min.js",
              "js/main.js"
            ],
      "all_frames": true
    }
  ],
  "web_accessible_resources": [ 
                                "js/jquery-2.0.3.min.js",
                                "js/jquery-2.0.3.min.map",
                                "sprite-reddit.png"
                              ]
}